public class ArthmaticCalculations {
    public Integer Addition(integer a, integer b){
        integer sum = a + b;
        return sum;
    }

    public integer Subtraction(integer a, integer b){
        integer sub = a - b;
        return sub;
    }

    public integer Multiplication(integer a, integer b, integer c){
        integer mul = a*b*c;
        return mul;
    }    
}